		                        dim MotifModel As New Motif
		                        MotifModel.fasta= Fasta
		                        MotifModel.mode=ModeFolder.Name
		                        MotifModel.TFname=Folder.Name
		                        MotifModel.number=n
		                        TFmotifsFasta.Append(MotifModel)
		                      End
		                    Next
		                  End
		                End
		              Next
		              f = Family.Child("hmmsearch_result_withCRtags.txt")
