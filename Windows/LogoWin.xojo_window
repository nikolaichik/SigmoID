		          For Each Folder As FolderItem in MEMEfolder.Children
		            If Folder.IsFolder Then
		              For Each ModeFolder As FolderItem in Folder.Children
		                If ModeFolder.IsFolder Then
		                  MEMEresult = ModeFolder.Child("meme.txt")
